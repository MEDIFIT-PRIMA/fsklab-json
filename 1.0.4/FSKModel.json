{
    "$id": "https://medifit-prima.github.io/fsklab-json/1.0.4/FSKModel.json",
    "properties": {
        "modelType": {
            "type": "string",
            "enum": [
                "genericModel",
                "processModel",
                "riskModel",
                "qraModel",
                "dataModel",
                "exposureModel",
                "toxicologicalModel",
                "healthModel",
                "doseResponseModel",
                "predictiveModel",
                "otherModel",
                "consumptionModel"
            ]
        }
    },
    "allOf": [
        {
            "if": {
                "properties": {
                    "modelType": {
                        "const": "genericModel"
                    }
                }
            },
            "then": {
                "$ref": "genericModel.json"
            }
        },
        {
            "if": {
                "properties": {
                    "modelType": {
                        "const": "dataModel"
                    }
                }
            },
            "then": {
                "$ref": "dataModel.json"
            }
        },
        {
            "if": {
                "properties": {
                    "modelType": {
                        "const": "predictiveModel"
                    }
                }
            },
            "then": {
                "$ref": "predictiveModel.json"
            }
        },
        {
            "if": {
                "properties": {
                    "modelType": {
                        "const": "otherModel"
                    }
                }
            },
            "then": {
                "$ref": "otherModel.json"
            }
        },
        {
            "if": {
                "properties": {
                    "modelType": {
                        "const": "exposureModel"
                    }
                }
            },
            "then": {
                "$ref": "exposureModel.json"
            }
        },
        {
            "if": {
                "properties": {
                    "modelType": {
                        "const": "toxicologicalModel"
                    }
                }
            },
            "then": {
                "$ref": "toxicologicalModel.json"
            }
        },
        {
            "if": {
                "properties": {
                    "modelType": {
                        "const": "doseResponseModel"
                    }
                }
            },
            "then": {
                "$ref": "doseResponseModel.json"
            }
        },
        {
            "if": {
                "properties": {
                    "modelType": {
                        "const": "processModel"
                    }
                }
            },
            "then": {
                "$ref": "processModel.json"
            }
        },
        {
            "if": {
                "properties": {
                    "modelType": {
                        "const": "consumptionModel"
                    }
                }
            },
            "then": {
                "$ref": "consumptionModel.json"
            }
        },
        {
            "if": {
                "properties": {
                    "modelType": {
                        "const": "healthModel"
                    }
                }
            },
            "then": {
                "$ref": "healthModel.json"
            }
        },
        {
            "if": {
                "properties": {
                    "modelType": {
                        "const": "riskModel"
                    }
                }
            },
            "then": {
                "$ref": "riskModel.json"
            }
        },
        {
            "if": {
                "properties": {
                    "modelType": {
                        "const": "qraModel"
                    }
                }
            },
            "then": {
                "$ref": "qraModel.json"
            }
        }
    ],
    "required": [
        "modelType"
    ]
}