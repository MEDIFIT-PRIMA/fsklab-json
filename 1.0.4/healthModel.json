{
    "$id": "https://medifit-prima.github.io/fsklab-json/1.0.4/healthModel.json",
    "definitions": {
        "type": "object",
        "properties": {
            "dataBackground": {
                "$ref": "/1.0.4/modelDataBackground.json"
            },
            "modelType": {
                "pattern": "^HealthModel$",
                "type": "string"
            },
            "generalInformation": {
                "$ref": "/1.0.4/genericModelGeneralInformation.json",
                "required": [
                    "name",
                    "identifier",
                    "creationDate",
                    "rights",
                    "reference",
                    "languageWrittenIn"
                ]
            },
            "modelMath": {
                "$ref": "/1.0.4/genericModelModelMath.json"
            },
            "scope": {
                "$ref": "/1.0.4/toxicologicalModelScope.json",
                "required": [
                    "hazard",
                    "populationGroup"
                ]
            }
        }
    }
}